{
    "version": 1,
    "name": "monad-protocol-dashboard",
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist"
        }
      }
    ],
    "routes": [
      {
        "src": "/testnet/(.*)",
        "dest": "/testnet/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }